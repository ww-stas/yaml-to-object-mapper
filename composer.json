    "minimum-stability": "stable",
    "keywords": [
        "yaml",
        "mapper"
    ],
    "require": {
        "php": ">=8",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
